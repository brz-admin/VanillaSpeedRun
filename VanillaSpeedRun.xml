<Ui xmlns="http://www.blizzard.com/wow/ui/" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/
	http://wowprogramming.com/FrameXML/UI.xsd">
	<Button name="VSR_frame" parent="UIParent" enableMouse="true" movable="true" frameStrata="LOW">
		<Scripts>
			<OnLoad>
				VSR_OnLoad()
			</OnLoad>
			<OnEvent>
				VSR_OnEvent()
			</OnEvent>
			<OnUpdate>
				VSR_OnUpdate(arg1)
			</OnUpdate>
			<OnDragStart>
				self:StartMoving()
			</OnDragStart>
			<OnDragStop>
				self:StopMovingOrSizing()
			</OnDragStop>
		</Scripts>
	</Button>
</Ui>